{
  "rewrites": [
    { "source": "/callback", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/old-path", "destination": "/new-path", "permanent": true }
  ],
  "headers": [
    { "source": "/api/*", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
